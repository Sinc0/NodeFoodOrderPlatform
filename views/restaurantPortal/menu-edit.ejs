                                <% } else { %>
                                    
                                <% } %>
                                <% } %>
                            </div>

                            <script>countCategories("<%=categoryFirstLetterUpperCase %>");</script>
                                
                        </div>
                    <% } %>
                    
                </form>
                <!-- form end -->
                
            </div>

